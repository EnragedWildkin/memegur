{
	"name": "memegur",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"test:update": "jest --updateSnapshot",
		"test:coverage": "jest --coverage",
		"lint": "eslint app/**/*.js __tests__/**/*.js; exit 0",
		"lint:fix": "eslint --fix app/**/*.js __tests__/**/*.js; exit 0"
	},
	"dependencies": {
		"apisauce": "^0.12.0",
		"native-base": "^2.1.3",
		"react": "16.0.0-alpha.6",
		"react-native": "0.43.4",
		"react-native-router-flux": "https://github.com/itinance/react-native-router-flux#fc1b8261144fe5f3132ac4940dd794ac06e9f351",
		"react-native-vector-icons": "^4.0.1",
		"react-redux": "^5.0.4",
		"redux": "^3.6.0",
		"redux-saga": "^0.15.3"
	},
	"devDependencies": {
		"babel-jest": "19.0.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react-native": "1.9.1",
		"babel-preset-stage-0": "^6.24.1",
		"coveralls": "^2.13.1",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^14.1.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^4.0.0",
		"eslint-plugin-react": "^6.10.3",
		"eslint-plugin-react-native": "^2.3.2",
		"jest": "19.0.2",
		"react-test-renderer": "16.0.0-alpha.6"
	},
	"jest": {
		"preset": "react-native",
		"transformIgnorePatterns": [
			"/node_modules/(?!native-base)/"
		],
		"collectCoverageFrom": [
			"app/**/*.{js}"
		]
	}
}
